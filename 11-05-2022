#include <iostream>
using namespace std;
int main(){
int angka[3][4] = {
{1, 2 , 4 , 5 },
{3 , 4 , 5 , 6 },
{5 , 6 , 4 , 4 },
};
cout<<angka[0][0]<<endl;
cout<<angka[0][1]<<endl;
cout<<angka[0][2]<<endl;
cout<<angka[0][3]<<endl;

cout<<angka[1][0]<<endl;
cout<<angka[1][1]<<endl;
cout<<angka[1][2]<<endl;
cout<<angka[1][3]<<endl;

cout<<angka[2][0]<<endl;
cout<<angka[2][1]<<endl;
cout<<angka[2][2]<<endl;
cout<<angka[2][3]<<endl;

    return 0;
}
